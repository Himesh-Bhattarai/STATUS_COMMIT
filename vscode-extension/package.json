{
  "name": "status-commit",
  "displayName": "STATUS Commit",
  "description": "Autocomplete and quick-pick for STATUS commit messages.",
  "version": "0.0.2",
  "publisher": "himeshchanchal",
  "license": "Apache-2.0",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Himesh-Bhattarai/STATUS_COMMIT.git"
  },
  "homepage": "https://github.com/Himesh-Bhattarai/STATUS_COMMIT#readme",
  "bugs": {
    "url": "https://github.com/Himesh-Bhattarai/STATUS_COMMIT/issues"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "SCM Providers",
    "Productivity"
  ],
  "keywords": [
    "git",
    "commit",
    "status",
    "convention",
    "scm"
  ],
  "activationEvents": [
    "onLanguage:git-commit",
    "onCommand:statusCommit.insertCode"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "statusCommit.insertCode",
        "title": "STATUS Commit: Insert Code"
      }
    ],
    "keybindings": [
      {
        "command": "statusCommit.insertCode",
        "key": "ctrl+alt+s",
        "mac": "cmd+alt+s",
        "when": "editorTextFocus || scmInputIsVisible"
      }
    ]
  }
}
